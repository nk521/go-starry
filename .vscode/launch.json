{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Launch starry",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}"
        },
        {
            "name": "Attach to Starry",
            "type": "go",
            "debugAdapter": "dlv-dap",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1",
            // "preLaunchTask": "Run headless dlv"
        }
    ],
}